<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="${jnlpSpec}" xmlns:jfx="http://javafx.com" href="${href}">
  <information>
    <title>${title}</title>
    <vendor>${vendor}</vendor>
    <description>${description}</description>
    [#if offlineAllowed]
    <offline-allowed/>
    [/#if]
  </information>
  <resources>
    <jfx:javafx-runtime version="${runtimeVersion}" href="${runtimeLocation}"/>
  </resources>
  <resources>
    <j2se version="${j2seVersion}" java-vm-args="${jvmArgs}" href="${j2seLocation}"/>
    [#list jnlpDependencies as jnlpDependency]
    <jar href="${jnlpDependency.name}" size="${jnlpDependency.size?c}" download="eager"/>
    [/#list]
  </resources>
  <applet-desc width="${width}" height="${height}" main-class="com.javafx.main.NoJavaFXFallback" name="${title}">
    <param name="requiredFXVersion" value="2.0"/>
    [#list jnlpParameters as jnlpParameter]
    <param name="${jnlpParameter.name}" value="${jnlpParameter.value}"/>
    [/#list]
    [#list jnlpArguments as jnlpArgument]
    <argument>${jnlpArgument}</argument>
    [/#list]
  </applet-desc>
  <jfx:javafx-desc width="${width}" height="${height}" main-class="${mainClass}" name="${title}"/>
  <update check="background"/>
</jnlp>
